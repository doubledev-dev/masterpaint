---
import Filter from "@/components/Filter/FilterContainer.astro";

const filterData = [
  {
    title: "Type",
    option: [
      "All","All","All","All","All","All","All"
    ]
  },
  {
    title: "Location",
    option: [
      "All","All","All","All","All","All","All"
    ]
  },
];
---

<div class="flex flex-row space-x-4 z-30">
    {
      filterData.map((choice)=>{
        return <Filter title={choice.title} option={choice.option} />
     })
    } 
</div>