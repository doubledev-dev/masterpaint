---
import MainLayout from "@/layouts/MainLayout.astro";
import { getCollection } from "astro:content";
import Projects from "@/components/Projects";

const projects = await getCollection("projects");
---

<MainLayout>
  <Projects
    slice={false}
    homePage={false}
    projectsPage={true}
    slugItems={projects}
    client:load
  />
</MainLayout>
